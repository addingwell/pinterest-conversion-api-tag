// We start by importing the necessary modules and defining the API version and endpoint.
const getAllEventData = require("getAllEventData");
const JSON = require("JSON");
const Math = require("Math");
const sendHttpRequest = require("sendHttpRequest");
const getTimestampMillis = require("getTimestampMillis");
const getContainerVersion = require("getContainerVersion");
const logToConsole = require("logToConsole");
const sha256Sync = require("sha256Sync");
const getRequestHeader = require("getRequestHeader");
const getType = require("getType");
const makeString = require("makeString");
const generateRandom = require("generateRandom");
const encodeUriComponent = require("encodeUriComponent");
const sendPixelFromBrowser = require("sendPixelFromBrowser");
const makeNumber = require("makeNumber");

const isLoggingEnabled = determinateIsLoggingEnabled();
const traceId = isLoggingEnabled ? getRequestHeader("trace-id") : undefined;

const eventData = getAllEventData();

// Define keys to be hashed
const keysToHash = [
  "em",
  "ph",
  "ge",
  "db",
  "ln",
  "fn",
  "ct",
  "st",
  "zp",
  "country",
  "hashed_maids",
  "external_id",
];

const EVENT_NAME_MAPPINGS = {
  add_to_cart: "add_to_cart",
  addtocart: "add_to_cart",
  purchase: "checkout",
  buy: "checkout",
  pay: "checkout",
  payment: "checkout",
  generate_lead: "lead",
  submit_application: "lead",
  contact: "lead",
  page_view: "page_visit",
  pageview: "page_visit",
  pagevisit: "page_visit",
  "gtm.dom": "page_visit",
  search: "search",
  find_location: "search",
  sign_up: "signup",
  signup: "signup",
  completeregistration: "signup",
  viewcategory: "view_category",
  viewcontent: "view_category",
  view_item_list: "view_category",
  watchvideo: "watch_video",
  add_payment_info: "custom",
  add_shipping_info: "custom",
  add_to_wishlist: "custom",
  begin_checkout: "custom",
  refund: "custom",
  remove_from_cart: "custom",
  select_item: "custom",
  select_promotion: "custom",
  view_cart: "custom",
  view_item: "page_visit",
  view_promotion: "custom",
};

const EVENT_NAME_PIXEL_MAPPINGS = {
  add_to_cart: "AddToCart",
  checkout: "Checkout",
  lead: "Lead",
  page_visit: "PageVisit",
  search: "Search",
  signup: "Signup",
  view_category: "ViewCategory",
  watch_video: "WatchVideo",
  custom: "Custom",
};

// Here we are constructing the request URL for the Pinterest API.
let postUrl = "https://api.pinterest.com/v5/ad_accounts/" + data.advertiserId + "/events";

let event = {};
event.event_name = getPinterestEventName(eventData.event_name);
event.action_source = data.actionSource || 'web';
event.event_source_url = (event.action_source === 'web') ? eventData.page_location : undefined;
event.event_time = Math.round(getTimestampMillis() / 1000);
event.event_id = eventData.event_id || generateEventId();
event.partner_name = "ss-addingwell";

// USER DATA
event.user_data = {};
event.user_data.client_ip_address = (event.action_source === 'web') ? eventData.ip_override : undefined;
event.user_data.client_user_agent = (event.action_source === 'web') ? eventData.user_agent : undefined;
event.user_data.external_id = eventData.user_id;
event.user_data.hashed_maids = eventData.hashed_maids;
event.user_data.click_id = eventData.click_id;
event.user_data.em = (eventData.user_data != null) ? eventData.user_data.email_address || eventData.user_data.sha256_email_address || eventData.user_data.email : undefined;
event.user_data.ph = (eventData.user_data != null) ? eventData.user_data.phone_number || eventData.user_data.sha256_phone_number : undefined;
event.user_data.ge = eventData.user_data != null ? eventData.user_data.gender : undefined;
event.user_data.db = eventData.user_data != null ? eventData.user_data.db : undefined;

// ADDRESS DATA
const addressData = (eventData.user_data != null && eventData.user_data.address != null) ? (eventData.user_data.address[0] || eventData.user_data.address) : {};
event.user_data.ln = addressData.last_name || addressData.sha256_last_name;
event.user_data.fn = addressData.first_name || addressData.sha256_first_name;
event.user_data.ct = addressData.city;
event.user_data.st = addressData.region;
event.user_data.zp = addressData.postal_code;
event.user_data.country = addressData.country;

// ECOMMERCE DATA
event.custom_data = {};
event.custom_data.currency = eventData.currency || ((eventData.items && eventData.items[0]) ? eventData.items[0].currency : undefined);
event.custom_data.value = (eventData.value) ? makeString(eventData.value) : getValueFromItems(eventData.items);
event.custom_data.content_ids = getContentIdsFromItems(eventData.items);
event.custom_data.content_name = (eventData.items && eventData.items.length === 1) ? eventData.items[0].item_name : undefined;
event.custom_data.content_category = (eventData.items && eventData.items.length === 1) ? eventData.items[0].item_category : undefined;
event.custom_data.content_brand = (eventData.items && eventData.items.length === 1) ? eventData.items[0].item_brand : undefined;
event.custom_data.contents = getContentFromItems(eventData.items);
event.custom_data.num_items = getItemsQuantity(eventData.items);
event.custom_data.order_id = makeString(eventData.transaction_id);
event.custom_data.search_string = eventData.search_term;

// OVERRIDE DATA WITH CONFIGURATION
if (data.serverEventDataList) {
  data.serverEventDataList.forEach((d) => {
    event[d.name] = d.value;
  });
}

if (data.userDataList) {
  data.userDataList.forEach((d) => {
    event.user_data[d.name] = d.value;
  });
}

if (data.customDataList) {
  data.customDataList.forEach((d) => {
    event.custom_data[d.name] = d.value;
  });
}

// CLEANUP / REMOVE EMPTY VALUES
let commonData = {};
for (let commonDataKey in event) {
  if (event[commonDataKey]) {
    commonData[commonDataKey] = event[commonDataKey];
  }
}
event = commonData;

if (event.user_data) {
  let userData = {};
  for (let userDataKey in event.user_data) {
    if (event.user_data[userDataKey]) {
      userData[userDataKey] = event.user_data[userDataKey];
    }
  }
  event.user_data = userData;
}

if (event.custom_data) {
  let customData = {};
  for (let customDataKey in event.custom_data) {
    if (
      event.custom_data[customDataKey] ||
      customDataKey === "value"
    ) {
      customData[customDataKey] = event.custom_data[customDataKey];
    }
  }
  event.custom_data = customData;
}

// HASH DATA
if (event.user_data) {
  for (let key in event.user_data) {
    if (keysToHash.indexOf(key) !== -1) {
      const hashedValue = hashAndReturnAsArray(
        key,
        event.user_data[key]
      );
      if (hashedValue !== undefined) {
        event.user_data[key] = hashedValue;
      }
    }
  }
}

const postBody = { data: [event] };

if (data.testMode) {
  postUrl = postUrl + "?test=true";
}

if (isLoggingEnabled) {
  logToConsole(
    JSON.stringify({
      Name: "Pinterest",
      Type: "Request",
      TraceId: traceId,
      EventName: event.event_name,
      RequestMethod: "POST",
      RequestUrl: postUrl,
      RequestBody: postBody,
    })
  );
}

// Now we are sending the HTTP request to the Pinterest API and handling the response.
sendHttpRequest(
  postUrl,
  (statusCode, headers, body) => {
    if (isLoggingEnabled) {
      logToConsole(
        JSON.stringify({
          Name: "Pinterest",
          Type: "Response",
          TraceId: traceId,
          EventName: event.event_name,
          ResponseStatusCode: statusCode,
          ResponseHeaders: headers,
          ResponseBody: body,
        })
      );
    }

    if (statusCode >= 200 && statusCode < 300) {

      if (!!data.sendPixelRequest) {

        let params = "?event=" + getPinterestPixelEventName(event.event_name) +
          addParam("tid", enc(data.tagId)) +
          addParam("event_id", event.event_id);

        if (event.custom_data) {
          params +=
            addParam("ed[value]", event.custom_data.value) +
            addParam("ed[order_quantity]", event.custom_data.num_items) +
            addParam("ed[currency]", event.custom_data.currency) +
            addParam("ed[order_id]", event.custom_data.order_id) +
            addParam("ed[search_query]", event.custom_data.search_string);
        }

        if (eventData.items) {
          for (var i = 0; i < eventData.items.length; i++) {
            params +=
              addParam("ed[line_items][" + i + "][product_name]", eventData.items[i].item_name) +
              addParam("ed[line_items][" + i + "][product_id]", getContentIdFromItem(eventData.items[i])) +
              addParam("ed[line_items][" + i + "][product_category]", eventData.items[i].item_category) +
              addParam("ed[line_items][" + i + "][product_variant]", eventData.items[i].item_variant) +
              addParam("ed[line_items][" + i + "][product_price]", eventData.items[i].price) +
              addParam("ed[line_items][" + i + "][product_quantity]", eventData.items[i].quantity) +
              addParam("ed[line_items][" + i + "][product_brand]", eventData.items[i].item_brand);
          }
        }

        if (event.user_data) {
          params +=
            addParam("pd[em]", event.user_data.em) +
            addParam("pd[ph]", event.user_data.ph) +
            addParam("pd[ge]", event.user_data.ge) +
            addParam("pd[db]", event.user_data.db) +
            addParam("pd[ln]", event.user_data.ln) +
            addParam("pd[fn]", event.user_data.fn) +
            addParam("pd[ct]", event.user_data.ct) +
            addParam("pd[st]", event.user_data.st) +
            addParam("pd[zp]", event.user_data.zp) +
            addParam("pd[country]", event.user_data.country) +
            addParam("cd[external_id]", event.user_data.external_id);
        }

        if (data.customDataList) {
          data.customDataList.forEach((d) => {
            params += addParam("ed[" + d.name + "]", d.value);
          });
        }

        sendPixelFromBrowser("https://ct.pinterest.com/v3/" + params);
      }

      data.gtmOnSuccess();
    } else {
      data.gtmOnFailure();
    }
  },
  {
    headers: {
      "content-type": "application/json",
      Authorization: "Bearer " + data.apiAccessToken,
    },
    method: "POST",
  },
  JSON.stringify(postBody)
);

// This function maps Google Analytics event names to Pinterest event names.
function getPinterestEventName(gtmEventName) {
  let pinterestEventName;

  if (data.eventType === "inherit") {
    // Check overwritten mapping first
    if (data.mappingEvents && data.mappingEvents.length > 0) {
      let overwrittenEvent;
      for (let i = 0; i < data.mappingEvents.length; i++) {
        if (data.mappingEvents[i].event === gtmEventName) {
          overwrittenEvent = data.mappingEvents[i];
        }
      }
      if (overwrittenEvent) {
        return overwrittenEvent.pinterestApiEventName;
      }
    }

    pinterestEventName =
      EVENT_NAME_MAPPINGS[gtmEventName.trim().toLowerCase()] || gtmEventName;
    return pinterestEventName;
  }

  return data.eventType === "standard"
    ? data.eventNameStandard
    : data.eventNameCustom;
}

// This function maps Pinterest API event names to Pinterest Pixel event names.
function getPinterestPixelEventName(pinterestApiEventName) {
  let pinterestEventName;

  // Check overwritten mapping first
  if (data.mappingEvents && data.mappingEvents.length > 0) {
    let overwrittenEvent;
    for (let i = 0; i < data.mappingEvents.length; i++) {
      if (
        data.mappingEvents[i].pinterestApiEventName === pinterestApiEventName
      ) {
        overwrittenEvent = data.mappingEvents[i];
      }
    }
    if (overwrittenEvent) {
      return overwrittenEvent.pinterestPixelEventName;
    }
  }

  pinterestEventName =
    EVENT_NAME_PIXEL_MAPPINGS[pinterestApiEventName] || pinterestApiEventName;
  return pinterestEventName;
}

function isAlreadyHashed(input) {
  return input && makeString(input).match("^[A-Fa-f0-9]{64}$") != null;
}

function hashData(key, input) {
  if (input == null || isAlreadyHashed(input)) {
    return input;
  }

  input = makeString(input).trim().toLowerCase();

  if (key === "ph") {
    input = input
      .split(" ")
      .join("")
      .split("-")
      .join("")
      .split("(")
      .join("")
      .split(")")
      .join("")
      .split("+")
      .join("");
  } else if (key === "ct") {
    input = input.split(" ").join("");
  }

  return sha256Sync(input, { outputEncoding: "hex" });
}

// Function to hash a value and return as array
function hashAndReturnAsArray(key, value) {
  const hashedValue = hashData(key, value);
  return [hashedValue];
}

function getItemField(defaultField) {
  let result = defaultField;

  const map = {
    item_id: 'id',
    price: 'item_price',
    quantity: 'quantity'
  };

  if (data.itemDataList) {
    data.itemDataList.forEach(d => {
      if (map[defaultField] === d.name) {
        result = d.field;
      }
    });
  }

  return result;
}

function getContentFromItems(items) {
  if (!items) return undefined;
  const idField = getItemField('item_id');
  const priceField = getItemField('price');
  const quantityField = getItemField('quantity');
  return items.map((item) => {
    return {
      id: makeString(item[idField]),
      item_price: makeString(item[priceField]),
      quantity: item[quantityField],
    };
  });
}

function getContentIdsFromItems(items) {
  if (!items) return undefined;
  const idField = getItemField('item_id');
  return items.map((item) => {
    return item[idField];
  });
}

function getContentIdFromItem(item) {
  if (!item) return undefined;
  const idField = getItemField('item_id');
    return item[idField];
}

function getItemsQuantity(items) {
  if (!items) return 0;
  let qty = 0;

  items.forEach((item) => {
    qty += item.quantity || 1;
  });

  return qty;
}

function getValueFromItems(items) {
  if (!items) return undefined;
  let value = 0;

  const priceField = getItemField('price');
  const quantityField = getItemField('quantity');

  items.forEach((item) => {
    let price = makeNumber(item[priceField]);
    value += item[quantityField] ? makeNumber(item[quantityField]) * price : price;
  });

  return makeString(value);
}

function generateEventId() {
  return getTimestampMillis() + "." + generateRandom(1000000000, 2147483647);
}

function determinateIsLoggingEnabled() {
  const containerVersion = getContainerVersion();
  return !!(
    containerVersion &&
    (containerVersion.debugMode || containerVersion.previewMode)
  );
}

function addParam(name, value) {
  if (value) return "&" + name + "=" + enc(value.toString());
  else return "";
}

function enc(data) {
  data = data || "";
  return encodeUriComponent(data);
}


